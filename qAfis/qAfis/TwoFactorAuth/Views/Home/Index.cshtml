</head>

<body id="page-top">


	<nav class="navbar navbar-expand-lg navbar-inverse fixed-top py-3 bg-dark" id="mainNav">
		<div class="container">
			<a class="navbar-brand js-scroll-trigger" href="#page-top">qAfis</a>
			<button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="navbarResponsive">
				<ul class="navbar-nav ml-auto my-2 my-lg-0">
					<li class="nav-item">
						<a class="nav-link js-scroll-trigger" href="#about">About</a>
					</li>
					<li class="nav-item">
						<a class="nav-link js-scroll-trigger" href="#services">Services</a>
					</li>
					<li class="nav-item">
						<a class="nav-link js-scroll-trigger" href="#team">Team</a>
					</li>
					<li class="nav-item">
						<a class="nav-link js-scroll-trigger" href="#contact">Contact</a>
					</li>
					@Html.Partial("_LoginPartial")
				</ul>
			</div>
		</div>
	</nav>

	<!-- Masthead -->
	<header class="masthead">
		<div class="container h-100">
			<div class="row h-100 align-items-center justify-content-center text-center">
				<div class="col-lg-10 align-self-end">
					<h1 class="text-uppercase text-white font-weight-bold">Your City, more safer than it's ever been !</h1>
					<hr class="divider my-4">
				</div>
				<div class="col-lg-8 align-self-baseline">
					<p class="text-white-75 font-weight-light mb-5">No need to worry about unidentified intruders casuing havoc in the city. You can now monitor all entry points and keep track of everyone coming in and leaving the city!</p>
					<a class="btn btn-primary btn-xl js-scroll-trigger" href="#about">Find Out More</a>
				</div>
			</div>
		</div>
	</header>

	<!-- About Section -->
	<section class="page-section bg-primary" id="about">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-lg-8 text-center">
					<h2 class="text-white mt-0">We've got your security needs covered!</h2>
					<hr class="divider light my-4">
					<p class="text-white-50 mb-4">qAfis provides a simple, yet effective solution to keep your city safe. Record fingerprints of all incoming and outgoing individuals and store them in our database. Keeping track of these individuals will help you investigate and flag any unwanted guests!</p>
					<a class="btn btn-light btn-xl js-scroll-trigger" href="#services">Get Started!</a>
				</div>
			</div>
		</div>
	</section>

	<!-- Services Section -->
	<section class="page-section" id="services">
		<div class="container">
			<h2 class="text-center mt-0">At Your Service</h2>
			<hr class="divider my-4">
			<div class="row">
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-gem text-primary mb-4"></i>
						<h3 class="h4 mb-2">Enroll</h3>
						<p class="text-muted mb-0">Enroll new fingerprints in your database!</p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/mortals/Create">Enroll Now!</a>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-laptop-code text-primary mb-4"></i>
						<h3 class="h4 mb-2">Verify</h3>
						<p class="text-muted mb-0">Verify if the fingerprint exists in the database!</p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/mortals/Verify">Verify Now!</a>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-globe text-primary mb-4"></i>
						<h3 class="h4 mb-2">Match</h3>
						<p class="text-muted mb-0">Compare two given fingerprints and check if they match!</p>
						<p class="text-muted mb-0">  </p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/mortals/Match">Match Now!</a>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-heart text-primary mb-4"></i>
						<h3 class="h4 mb-2">Database</h3>
						<p class="text-muted mb-0">Review, Manage and Edit your database!</p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/mortals/Index">View Database!</a>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-3 offset-2 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-address-book text-primary mb-4"></i>
						<h3 class="h4 mb-2">Logs</h3>
						<p class="text-muted mb-0">Review, Manage and Edit your Logs!</p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/logs/Index">Check your Logs!</a>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-address-book text-primary mb-4"></i>
						<h3 class="h4 mb-2">Bulk Testing</h3>
						<p class="text-muted mb-0">Bulk Test your Prints!</p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/mortals/BulkCreate">Bulk Test the Prints!</a>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<i class="fas fa-4x fa-address-book text-primary mb-4"></i>
						<h3 class="h4 mb-2">Manupilated Testing</h3>
						<p class="text-muted mb-0">Test your Manupilated Prints!!</p>
						<a class="btn btn-light btn-xl js-scroll-trigger" href="~/mortals/ManCreate">Check Manupilated Prints!</a>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Portfolio Section -->
	<!-- Team Section -->
	<section class="page-section bg-light" id="team">
		<div class="container">
			<h2 class="text-center mt-0">The Team</h2>
			<hr class="divider my-4">
			<div class="row">
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<img src="~/Content/startbootstrap-creative-gh-pages (1)/startbootstrap-creative-gh-pages/img/circle-cropped (2).png" style="height: 250px;width: 250px;display: block;align-content:center; margin-bottom:50px">
						<h3 class="h4 mb-2">AbdulRafay Mehboob</h3>

					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<img src="~/Content/startbootstrap-creative-gh-pages (1)/startbootstrap-creative-gh-pages/img/circle-cropped.png" style="height: 250px;width: 250px;display: block;align-content:center; margin-bottom:50px">
						<h3 class="h4 mb-2">Aniqa Ahsan</h3>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<img src="~/Content/startbootstrap-creative-gh-pages (1)/startbootstrap-creative-gh-pages/img/circle-cropped (1).png" style="height: 250px;width: 250px;display: block;align-content:center; margin-bottom:50px">
						<h3 class="h4 mb-2">Ismail Vohra</h3>
					</div>
				</div>
				<div class="col-lg-3 col-md-6 text-center">
					<div class="mt-5">
						<img src="~/Content/startbootstrap-creative-gh-pages (1)/startbootstrap-creative-gh-pages/img/circle-cropped (3).png" style="height: 250px;width: 250px;display: block;align-content:center; margin-bottom:50px">
						<h3 class="h4 mb-2">Saman Gaziani</h3>
					</div>
				</div>
			</div>
		</div>
	</section>

